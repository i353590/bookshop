using MY_BOOKSHOP_AUTHORS as MY_BOOKSHOP_AUTHORS;
using LOCALIZED_MY_BOOKSHOP_BOOKS as LOCALIZED_MY_BOOKSHOP_BOOKS;
view LOCALIZED_MY_BOOKSHOP_AUTHORS as select from MY_BOOKSHOP_AUTHORS as L mixin {
  ___BOOKS : association[*] to LOCALIZED_MY_BOOKSHOP_BOOKS on ___BOOKS.AUTHOR_ID = $projection.ID;
} into {
  L.CREATEDAT,
  L.CREATEDBY,
  L.MODIFIEDAT,
  L.MODIFIEDBY,
  key L.ID as ID,
  L.NAME,
  ___BOOKS as BOOKS
};
